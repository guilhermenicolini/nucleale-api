table(cellspacing='0' cellpadding='0' style='width: 100%; border-collapse: collapse;')
  tr
    td(style='text-align: center; padding: 3px;' colspan='4')
      b PRESTADOR DE SERVIÇOS
  tr
    td(style='width: 10%; padding: 3px;' rowspan='5')
      div(style='display: flex; justify-content: center; align-items: center;')
        include promedicin.pug
    td(style='padding: 3px;' colspan='3')
      label(style='padding-right: 3px;') Nome/Razão Social:
      span
        b #{provider.name}
  tr
    td(style='padding: 3px;')
      label(style='padding-right: 3px;') CPF/CNPJ:
      span
        b #{provider.taxId}
    td(style='padding: 3px;' colspan='2')
      label(style='padding-right: 3px;') Inscrição Municipal:
      span
        b #{provider.registryId}
  tr
    td(style='padding: 3px;' colspan='3')
      label(style='padding-right: 3px;') Endereço:
      span
        b #{provider.address}
  tr
    td(style='width: 40%;padding: 3px;')
      label(style='padding-right: 3px;') Município:
      span
        b #{provider.city}
    td(style='width: 30%;padding: 3px;')
      label(style='normal; padding-right: 3px;') UF:
      span
        b #{provider.state}
    td(style='width: 20%;padding: 3px;')
      label(style='padding-right: 3px;') Telefone:
      span
        b #{provider.phone}
